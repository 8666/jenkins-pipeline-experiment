pipeline {
    agent any
    stages {
        stage('Example Build') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Example Deploy') {
            when { branch 'main' }
            }
            steps {
                echo 'Deploying'
                echo "${BRANCH_NAME}"
                MY_NAME="prod"
            }
        }
        stage('Final') {
            steps {
                echo "${MY_NAME}"

            }
        }
    }
}
